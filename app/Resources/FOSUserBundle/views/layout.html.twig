    <div id="main" role="main">

        <!-- MAIN CONTENT -->
        <div id="content" class="container">

            <div class="row">

                <div class="col-xs-12 col-sm-12 col-md-6 col-lg-4 col-md-offset-3 col-md-offset-4">
                    <br><br>
                    <div class="well no-padding">

                            {% if app.request.hasPreviousSession %}

                                {% for type, messages in app.session.flashbag.all() %}

                                    {% for message in messages %}

                                        <div class="flash-{{ type }}">
                                            {{ message }}
                                        </div>

                                    {% endfor %}

                                {% endfor %}
                            {% endif %}

                            <div>
                                {% block fos_user_content %}
                                {% endblock fos_user_content %}
                            </div>


                    </div>





                </div>
                <div class="col-xs-12 col-sm-12 col-md-6 col-lg-4 col-md-offset-3 col-md-offset-4">
                    <a class="pull-left" href="{{ path('register_route') }}">Registration </a>
                    <a  class="pull-right" href="{{ path('fos_user_resetting_request') }}">Forgot password ?</a>
                </div>
            </div>
        </div>


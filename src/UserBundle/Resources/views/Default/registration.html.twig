    <script type="text/javascript">

        // DO NOT REMOVE : GLOBAL FUNCTIONS!

        $(document).ready(function() {
            var availableTags=[];
            {% for entity in zip %}
                 availableTags.push('{{ entity.subDisNameTh }}'+','+'{{ entity.disNameTh }}'+','+'{{ entity.prvNameTh }}'+','+'{{ entity.zipCode }}');
            {% endfor %}

            $('#addressAuto').autocomplete({
                source:availableTags,
                selectFirst: true, //here
                minLength: 5,
                select: function( event, ui ) {
                    console.log( ui.item ?
                        "Selected: " + ui.item.value + " aka " + ui.item.id ://this throw undefined
                        "Nothing selected, input was " + this.value );

                    //alert(ui.item.value);
                    var arrayVal=ui.item.value.split(',');
                    var subDisNamth=arrayVal[0];
                    var disNameTh=arrayVal[1];
                    var prvNameTh=arrayVal[2];
                    var zipcode=arrayVal[3];

                    //  $('#form_prvCode').
                    $("#form_prvCode option").filter(function() {
                        return this.text == prvNameTh;
                    }).attr('selected', true);
                    $("#form_disCode option").filter(function() {
                        return this.text == disNameTh;
                    }).attr('selected', true);
                    $("#form_subDisCode option").filter(function() {
                        return this.text == subDisNamth;
                    }).attr('selected', true);
                    $('#form_zipCode').val(zipcode);
                }
            });
        });

    </script>

